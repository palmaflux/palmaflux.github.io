//<![CDATA[
jQuery(document).ready(function(){
   $('#slogan-main').mousemove(function(e){
     var rXP = (e.pageX - this.offsetLeft-$(this).width()/2);
     var rYP = (e.pageY - this.offsetTop-$(this).height()/2);
     $('#slogan-main').css('text-shadow', +rYP/24+'px '+rXP/32+'px rgba(227,6,19,118), '+rYP/8+'px '+rXP/60+'px rgba(255,237,0,1), '+rXP/70+'px '+rYP/12+'px rgba(0,159,227,27)');
   });
});
//]]>
</script>

<script>
//<![CDATA[ 
//top menu scroll
$(document).ready(function(){
  var scrollTop = 0;
  $(window).scroll(function(){
    scrollTop = $(window).scrollTop();
     $('.count').html(scrollTop);
    if (scrollTop >= 120) {
      $('#nav-top').addClass('nav-scroll');
    } else if (scrollTop < 120 ) {
      $('#nav-top').removeClass('nav-scroll');
    } 
  }); 
});
//]]>
</script>

